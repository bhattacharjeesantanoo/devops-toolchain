[project]
	description = sample webcode
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/bhattacharjeesantanoo
[access "refs/for/refs/*"]
	read = group user/bhattacharjeesantanoo
	push = group user/bhattacharjeesantanoo
	label-Code-Review = -2..+2 group user/bhattacharjeesantanoo
	label-Verified = -1..+1 group user/bhattacharjeesantanoo
	rebase = group user/bhattacharjeesantanoo
	removeReviewer = group user/bhattacharjeesantanoo
	submit = group user/bhattacharjeesantanoo
	exclusiveGroupPermissions = read push label-Code-Review label-Verified rebase removeReviewer submit
[access "refs/heads/*"]
	read = group user/bhattacharjeesantanoo
	create = group user/bhattacharjeesantanoo
	push = +force group user/bhattacharjeesantanoo
	pushMerge = group user/bhattacharjeesantanoo
	exclusiveGroupPermissions = read create push pushMerge
[access "refs/tags/*"]
	push = +force group user/bhattacharjeesantanoo
	push = +force group user/bhattacharjeesantanoo
	exclusiveGroupPermissions = push
